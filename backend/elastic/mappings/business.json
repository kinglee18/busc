  "settings": {
    "index": {
      "analysis": {
        "filter": {
          "cities_synonym": {
            "type": "synonym",
            "synonyms_path": "cities.txt"
          },
          "colonies_synonym": {
            "type": "synonym",
            "synonyms_path": "colonies.txt"
          },
          "states_synonym": {
            "type": "synonym",
            "synonyms_path": "states.txt"
          },
          "spanish_stop_filter": {
            "type": "stop",
            "stopwords": "_spanish_"
          },
          "synonym": {
            "type": "synonym",
            "synonyms_path": "synonym.txt"
          },
          "unique_filter": {
            "type": "unique",
            "only_in_same_position": "true"
          },
          "stop_words_filter": {
            "type": "stop",
            "stopwords": [
              "alguna",
              "alguno",
              "ambos",
              "aqui",
              "arriba",
              "bajo",
              "cada",
              "dentro",
              "fin",
              "hacemos",
              "hacen",
              "hacer",
              "intento",
              "largo",
              "mio",
              "modo",
              "podemos",
              "poder",
              "puede",
              "pueden",
              "puedo",
              "sabe",
              "sabemos",
              "saben",
              "saber",
              "sabes",
              "ser",
              "solamente",
              "solo",
              "tener",
              "tiempo",
              "tras",
              "ultimo",
              "usa",
              "usamos",
              "usan",
              "usar",
              "usas",
              "uso",
              "va",
              "valor",
              "vamos",
              "van",
              "vaya",
              "verdad",
              "verdadera",
              "cabe",
              "hacia",
              "salvo",
              "segun",
              "so",
              "aborto",
              "abortos",
              "legrado"
            ]
          },
          "asciifolding_filter_two": {
            "type": "asciifolding",
            "preserve_original": "true"
          },
          "spanish_stemmer": {
            "type": "stemmer",
            "language": "spanish"
          },
          "asciifolding_filter": {
            "type": "asciifolding",
            "preserve_original": "true"
          },
          "autocomplete_filter": {
            "max_shingle_size": "5",
            "min_shingle_size": "3",
            "type": "shingle"
          },
          "custom_stems": {
            "type": "stemmer_override",
            "rules_path" : "stemmer_override.txt"
          }
        },
        "analyzer": {
          "phones": {
            "char_filter": [
              "only_numbers"
            ],
            "tokenizer": "standard"
          },
          "cities_analyzer": {
            "filter": [
              "asciifolding_filter",
              "lowercase",
              "cities_synonym"
            ],
            "tokenizer": "standard"
          },
          "states_analyzer": {
            "filter": [
              "asciifolding_filter",
              "lowercase",
              "states_synonym"
            ],
            "tokenizer": "standard"
          },
          "colonies_analyzer": {
            "filter": [
              "asciifolding_filter",
              "lowercase",
              "colonies_synonym"
            ],
            "tokenizer": "standard"
          },
          "keyword_analyzer": {
            "filter": [
              "asciifolding_filter",
              "lowercase"
            ],
            "ignore_above": "256",
            "tokenizer": "keyword"
          },
          "keyword_analyzer_dos": {
            "filter": [
              "lowercase",
              "asciifolding_filter_two"
            ],
            "char_filter": "multispaces",
            "tokenizer": "keyword"
          },
          "spanish_analyzer": {
            "filter": [
              "custom_stems",
              "spanish_stemmer",
              "synonym",
              "apostrophe",
              "asciifolding_filter",
              "lowercase",
              "spanish_stop_filter",
              "stop_words_filter",
              "unique_filter"
            ],
            "tokenizer": "standard"
          }
        },
        "char_filter": {
          "multispaces": {
            "pattern": " +",
            "type": "pattern_replace",
            "replacement": " "
          },
          "only_numbers": {
            "pattern": "(\\D)",
            "type": "pattern_replace",
            "replacement": ""
          }
        }
      }
    }
  }
